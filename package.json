{
  "name": "antique",
  "version": "1.0.0",
  "bundleIdentifier": "com.webnum.antique",
  "main": "index.js",
  "scripts": {
    "start": "npx expo start --clear",
    "dev": "npx expo start",
    "dev:ios": "concurrently -n metro,ios \"EXPO_OFFLINE=1 npx expo start --clear\" \"wait-on tcp:127.0.0.1:8081 -t 60000 && sleep 8 && CI=false npx expo run:ios\"",
    "ios:sim": "CI=false npx expo run:ios --device 'iPhone 15'",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "web": "npx expo start --web",
    "test": "jest",
    "increment-build": "node scripts/increment-build.js",
    "build:apk": "npx eas build --platform android --profile preview",
    "build:ios": "npm run increment-build && npx eas build --platform ios --profile production",
    "build:ios:no-submit": "npx eas build --platform ios --profile production",
    "build:ios:preview": "npm run increment-build && npx eas build --platform ios --profile preview",
    "submit:ios": "npx eas submit --platform ios --profile production",
    "deploy:testflight": "npm run increment-build && npx eas build --platform ios --profile production --auto-submit",
    "dev:go": "npx expo start --clear --ios",
    "dev:ios:metro": "concurrently -n metro,ios \"EXPO_OFFLINE=1 npx expo start --clear\" \"wait-on tcp:127.0.0.1:8081 -t 60000 && CI=false npx expo run:ios\""
  },
  "dependencies": {
    "@expo-google-fonts/albert-sans": "^0.4.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.14.0",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.13.0",
    "@supabase/supabase-js": "^2.97.0",
    "babel-preset-expo": "~54.0.10",
    "expo": "~54.0.33",
    "expo-auth-session": "~7.0.10",
    "expo-blur": "~15.0.8",
    "expo-camera": "~17.0.10",
    "expo-document-picker": "~14.0.3",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.11",
    "expo-haptics": "~15.0.8",
    "expo-image-manipulator": "~14.0.3",
    "expo-image-picker": "~17.0.10",
    "expo-linking": "~8.0.11",
    "expo-media-library": "~18.2.1",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~15.0.10",
    "phosphor-react-native": "^3.0.3",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "zustand": "^5.0.11"
  },
  "private": true,
  "devDependencies": {
    "@types/react": "~19.1.10",
    "concurrently": "^9.2.1",
    "typescript": "~5.9.2",
    "wait-on": "^9.0.4"
  },
  "overrides": {
    "@babel/helper-validator-option": "7.27.1"
  }
}
