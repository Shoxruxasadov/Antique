{
  "expo": {
    "name": "AntiqPro",
    "slug": "AntiqPro",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "com.webnum.antique",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/Splash_Screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#E3D9CB"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.webnum.antique",
      "usesAppleSignIn": true
    },
    "android": {
      "icon": "./assets/logo.png",
      "edgeToEdgeEnabled": true,
      "statusBar": {
        "translucent": true,
        "backgroundColor": "#00000000"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO"
      ],
      "package": "com.webnum.antique"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow AntiqPro to access your camera"
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow AntiqPro to access your photos for the last photo preview."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow AntiqPro to access your photos."
        }
      ],
      "expo-web-browser",
      "expo-localization"
    ],
    "owner": "webnum.com",
    "extra": {
      "eas": {
        "projectId": "e8f67aa9-ebfb-4dcc-8062-b8a8ee631705"
      }
    }
  }
}
